<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>PoJun.Dapper.Repository</PackageId>
    <PackageTags>orm;sql;micro-orm</PackageTags>
    <Title>Dapper</Title>
    <Description>基于Dapper二次封装的ORM</Description>
    <Authors>PoJun</Authors>
    <TargetFrameworks>net452;net46;net47;netstandard2.0;netcoreapp2.1</TargetFrameworks>
    <Company>PoJun.Dapper.Repository</Company>
    <Product>PoJun.Dapper.Repository</Product>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <AssemblyVersion>3.0.0.6</AssemblyVersion>
    <FileVersion>3.0.0.6</FileVersion>
    <Version>3.0.6</Version>
    <PackageProjectUrl>https://github.com/YGeneral/PoJun.Dapper</PackageProjectUrl>
    <Copyright>PoJun</Copyright> 
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>bin\Release\netstandard2.0\PoJun.Dapper.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Dapper" Version="1.60.6" />
    <PackageReference Include="MySql.Data" Version="8.0.20" />
  </ItemGroup>

</Project>
